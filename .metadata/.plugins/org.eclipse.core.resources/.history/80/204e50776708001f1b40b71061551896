package com.work.community.repository;

import java.time.LocalDateTime;
import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.work.community.entity.Event;

@Repository
public interface EventRepository extends JpaRepository<Event, Integer>{

	List<Event> findByStartDateTimeBetweenAndEndDateTimeBetween
	(LocalDateTime startDateTime, LocalDateTime endDateTime, LocalDateTime startDateTime2, LocalDateTime endDateTime2);

}
